#include <iostream>
#include <string.h>
using namespace std;

struct point {
    int x, y;
};

struct node {
	point info;
	node* next;
};

struct List {
	node* head;
	node* tail;
};
void CreateEmptyList(List &L)
{
    L.head =NULL;
    L.tail =NULL;
}
node* creatNode (int x,int y)
{
    node *p =new node;
    p->info.x =x;
    p->info.y =y;
    p->next =NULL;
    return p;
}
void AddNode(List &L, node* p)
{
    if (L.head ==NULL)
    {
        L.head =p;
        L.tail = L.head;
    }
    else
    {
    L.tail->next =p;
    L.tail=p;
    }
}
void CreateList(List &L)
{
    int n;
    cin >> n;
    int x,y;
    node* p;
    for (int i =0 ;i < n; i++)
    {
        cin >>x >> y;
        p= creatNode(x , y);
        AddNode(L,p);
    }

}
void sort()
void PrintList(List &L)
{
    for (auto p =L.head; p != NULL; p = p->next)
    {
        cout<< "(" << p->info.x<< "," << p->info.y<<")\n";
    }
}
int main() {
    List L;
	CreateEmptyList(L);
	CreateList(L);
	PrintList(L);
    return 0;
}
